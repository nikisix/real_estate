            Y.on('searchMain:setListHTML', function (e) {
                Y.each(unitLists, function (value) {
                    //TODO tear down each Y.Z.BuildingList here.
                }, this);
                unitLists = {};
            });

            function onBuildingHover(e) {
                initUnitList(e.currentTarget);
            }

            function onBuildingLinkClick(e) {
                e.preventDefault();
                initUnitList(e.currentTarget.ancestor('article'), true);
            }

            function initUnitList(buildingArticle, visibleState) {
                var zpid = buildingArticle.get('id').replace('zpid_', '');

                if (!unitLists[zpid]) {
                    unitLists[zpid] = new Y.Z.BuildingList({
                        triggerNode: buildingArticle.all('.trigger-units'),
                        preBoundingBox: buildingArticle.one('.unit-list'),
                        visible: false,
                        seoList: false,
                        buildingLat: buildingArticle.getAttribute('latitude') / 1000000,
                        buildingLong: buildingArticle.getAttribute('longitude') / 1000000,
                        totalUnitCount: buildingArticle.getAttribute('unitCount'),
                        showUnitsNumber: buildingArticle.getAttribute('showUnitsNumber'),
                        searchUrl: window.location.pathname,
                        builderCommunity: buildingArticle.hasClass('builder-community')
                    }).render(
                        buildingArticle.one('.property-info')
                    );

                    if (visibleState) {
                        unitLists[zpid].set('visible', true);
                        unitLists[zpid]._getUnits();
                    }
                }
            }
        
